{
  "DataBase": {
    "ConnectionString": ""
  },
  "JwtOptions": {
    "SecretKey": "",
    "AccessExpiresHours": "1",
    "RefreshTokenExpiresHours": "168",
    "NameCookie": "Super-Puper-Cookie",
    "Audience": "Audience",
    "Issue": "Issue"
  },
  "TelegramOptions": {
    "SecretKey": ""
  },
  "TracingConfiguration": {
    "Url": "http://tg-jaeger:4318/v1/traces"
  },
  "OpenRouterOptions": {
    "SecretKey": ""
  },
  "CorsPolicies": {
    "AllowedOrigins": [
      "http://localhost:5173"
    ]
  },
  "S3Options": {
    "AccessKey": "",
    "SecretKey": "",
    "ServiceUrl": "",
    "BucketName": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/app/logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://loki:3100",
          "restrictedToMinimumLevel": "Verbose",
          "propertiesAsLabels": [
            "Application",
            "level"
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "api"
    }
  }
}