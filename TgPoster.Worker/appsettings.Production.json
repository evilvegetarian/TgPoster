{
  "TelegramOptions": {
    "SecretKey": ""
  },
  "DataBase": {
    "ConnectionString": ""
  },
  "TelegramSettings": {
    "api_id": "",
    "api_hash": "",
    "phone_number": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://your-loki-url:3100",
          "restrictedToMinimumLevel": "Verbose",
          "propertiesAsLabels": [
            "Application",
            "level"
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "YourAppName"
    }
  }
}
